;AAAA,GANA;AAkBA,EAAA,OAAA,EAAA;AACA,IAAA,eADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,cAAA,QAFA,GAEA;AACA,gBAAA,IAAA,EAAA,KAAA,aADA;AAEA,gBAAA,IAAA,EAAA,KAAA;AAFA,eAFA;AAAA;AAAA,4BAMA,aAAA,CAAA,QAAA,CANA;AAAA,4BAOA,KAAA,QAAA,GAAA,IAPA;AAAA,4BAQA,KAAA,gBAAA,GAAA,KAAA,aARA;AAAA;AAAA,4BASA,KAAA,CAAA,QAAA,CATA;AAAA,4BAUA,KAAA,QAAA,GAAA,IAVA;AAAA,4BAWA,KAAA,gBAAA,GAAA,KAAA,aAXA;AAAA,4BAYA,KAAA,CAAA,OAAA,CAZA;AAAA;AAAA,8CAaA,WAAA,CAAA;AAAA,gBAAA,GAAA,EAAA,KAAA,GAAA;AAAA,gBAAA,QAAA,EAAA,KAAA;AAAA,eAAA,CAAA,CAAA,IAAA,CACA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,oCAAA,CADA,CAbA;;AAAA;AAAA;AAAA,yCASA,IATA;AAAA;AAAA;;AAAA;AAAA;AAAA,yCAMA,IANA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA,IAAA,YAnBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBA,cAAA,QApBA,GAoBA;AACA,gBAAA,IAAA,EAAA,KAAA,UADA;AAEA,gBAAA,IAAA,EAAA,KAAA;AAFA,eApBA;AAAA;AAAA,8CAwBA,KAAA,CAAA,QAAA,CAxBA;;AAAA;AAAA;AAAA;AAAA;AAAA;;AAyBA,cAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,WAAA;AACA,mBAAA,QAAA,GAAA,IAAA;AACA,mBAAA,gBAAA,GAAA,KAAA,UAAA;AACA,mBAAA,aAAA,GAAA,KAAA;AA5BA;AAAA;;AAAA;AA8BA,cAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,mBAAA,aAAA,GAAA,IAAA;;AA/BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmCA,IAAA,MAnCA;AAAA;AAAA;AAAA;AAAA;AAoCA,cAAA,QAAA,CAAA,EAAA,CAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,MAAA;;AArCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GAlBA;AA0DA,EAAA,OA1DA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4CA4DA,SAAA,EA5DA;;AAAA;AA4DA,YAAA,GA5DA;AA6DA,iBAAA,QAAA,GAAA,IAAA;AACA,iBAAA,gBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AA9DA;AAAA;;AAAA;AAAA;AAAA;AAgEA,iBAAA,QAAA,GAAA,KAAA;AACA,iBAAA,gBAAA,GAAA,WAAA;;AAjEA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,CAAA","sourcesContent":["<template>\n  <div align=\"center\">\n    <div v-if=\"loggedIn\">\n      <span class=\"headline\">Logged in as {{this.loggedInUserName}}</span>\n      <v-btn class=\"mr-4\" @click=\"logOut\">Logout</v-btn>\n    </div>\n\n    <div class=\"LoginRegistration\" v-if=\"!loggedIn\">\n      <h1>Registration and Login</h1>\n      <br />\n      <v-container>\n        <v-layout row>\n          <v-spacer></v-spacer>\n          <v-row>\n            <v-card max-width=\"1000\" min-width=\"400\">\n              <v-card-title>Register</v-card-title>\n              <v-container>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"name_register\"\n                  label=\"Name\"\n                  required\n                ></v-text-field>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"password_register\"\n                  :type=\"'password'\"\n                  label=\"Password\"\n                  required\n                ></v-text-field>\n                <v-text-field clearable dense filled outlined v-model=\"age\" label=\"Age\" required></v-text-field>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"favoriteAlbum\"\n                  label=\"Favorite Album\"\n                  required\n                ></v-text-field>\n              </v-container>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn class=\"mr-4\" @click=\"registerAccount\">Register</v-btn>\n                <v-spacer></v-spacer>\n              </v-card-actions>\n            </v-card>\n          </v-row>\n\n          <v-spacer></v-spacer>\n\n          <v-row>\n            <v-card max-width=\"1000\" min-width=\"400\">\n              <v-card-title>Login</v-card-title>\n              <v-container>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"name_login\"\n                  label=\"Name\"\n                  required\n                ></v-text-field>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"password_login\"\n                  :type=\"'password'\"\n                  label=\"Password\"\n                  required\n                ></v-text-field>\n              </v-container>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn class=\"mr-4\" @click=\"loginAccount\">Login</v-btn>\n                <v-spacer></v-spacer>\n              </v-card-actions>\n            </v-card>\n          </v-row>\n        </v-layout>\n        <br>\n        <v-alert type='error' v-if=\"loggedInError\">\n          Error logging in\n        </v-alert>\n      </v-container>\n    </div>\n    <Search :loggedInUserName=\"this.loggedInUserName\" />\n  </div>\n</template>\n\n<script>\nimport { createAccount } from \"../api/account/Account\";\nimport { login } from \"../api/account/Account\";\nimport { getStatus } from \"../api/account/Account\";\nimport { addUserInfo } from \"../api/account/User\";\nimport { setToken } from \"../config/Token\";\nimport Search from \"@/components/Search.vue\";\n\nexport default {\n  name: \"LoginRegister\",\n\n  components: {\n    Search\n  },\n  data: () => ({\n    name_register: \"\",\n    password_register: \"\",\n    name_login: \"\",\n    password_login: \"\",\n    error: \"\",\n    loggedIn: false,\n    loggedInUserName: \"\",\n    favoriteAlbum: \"\",\n    age: \"\",\n    loggedInError: false\n  }),\n  methods: {\n    async registerAccount() {\n      const namePass = {\n        name: this.name_register,\n        pass: this.password_register\n      };\n      await createAccount(namePass).then(\n        (this.loggedIn = true),\n        (this.loggedInUserName = this.name_register),\n        await login(namePass).then(\n          (this.loggedIn = true),\n          (this.loggedInUserName = this.name_register),\n          alert(\"pause\"),\n          await addUserInfo({ age: this.age, favAlbum: this.favoriteAlbum }).then(\n            window.console.log(\"manged to add user info, hopefully\")\n          )\n        )\n      );\n    },\n    async loginAccount() {\n      const namePass = {\n        name: this.name_login,\n        pass: this.password_login\n      };\n      if (await login(namePass)) {\n        window.console.log(\"logged in\");\n        this.loggedIn = true;\n        this.loggedInUserName = this.name_login;\n        this.loggedInError = false;\n      } else {\n        window.console.log(\"failed\");\n        this.loggedInError = true;\n      }\n\n    },\n    async logOut() {\n      setToken(\"\");\n      window.location.reload();\n    }\n  },\n  async created() {\n    try {\n      let res = await getStatus();\n      this.loggedIn = true;\n      this.loggedInUserName = res.user.name;\n    } catch (err) {\n      this.loggedIn = false;\n      this.loggedInUserName = \"Anonymous\";\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/components"}]}