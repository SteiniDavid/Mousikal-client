A,aADA;AAEA,gBAAA,IAAA,EAAA,KAAA;AAFA,eAFA;AAAA;AAAA,4BAMA,aAAA,CAAA,QAAA,CANA;AAAA,4BAOA,KAAA,QAAA,GAAA,IAPA;AAAA,4BAQA,KAAA,gBAAA,GAAA,KAAA,aARA;AAAA;AAAA,8CASA,KAAA,CAAA,QAAA,CAAA,CAAA,IAAA,CACA,KAAA,QAAA,GAAA,IADA,EAEA,KAAA,gBAAA,GAAA,KAAA,aAFA,CATA;;AAAA;AAAA;AAAA,wCAMA,IANA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA,IAAA,YAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA,cAAA,QAhBA,GAgBA;AACA,gBAAA,IAAA,EAAA,KAAA,UADA;AAEA,gBAAA,IAAA,EAAA,KAAA;AAFA,eAhBA;AAAA;AAAA,8CAoBA,KAAA,CAAA,QAAA,CApBA;;AAAA;AAAA;AAAA;AAAA;AAAA;;AAqBA,cAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,WAAA,GACA,KAAA,QAAA,GAAA,IADA,EAEA,KAAA,gBAAA,GAAA,KAAA,UAFA;AArBA;AAAA;;AAAA;AAyBA,cAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,QAAA;;AAzBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmCA,IAAA,MAnCA;AAAA;AAAA;AAAA;AAAA;AAoCA,cAAA,QAAA,CAAA,EAAA,CAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,MAAA;;AArCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GAfA;AAuDA,EAAA,OAvDA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4CAyDA,SAAA,EAzDA;;AAAA;AAyDA,YAAA,GAzDA;AA0DA,iBAAA,QAAA,GAAA,IAAA;AACA,iBAAA,gBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AA3DA;AAAA;;AAAA;AAAA;AAAA;AA6DA,iBAAA,QAAA,GAAA,KAAA;AACA,iBAAA,gBAAA,GAAA,EAAA;;AA9DA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,CAAA","sourcesContent":["<template>\n  <div align=\"center\">\n    <div v-if=\"loggedIn\">\n      <span class=\"headline\">Logged in as {{this.loggedInUserName}} </span>\n      <v-btn class=\"mr-4\" @click=\"logOut\">Logout</v-btn>\n      <Search :loggedInUserName=\"this.loggedInUserName\" />\n    </div>\n\n    <div class=\"LoginRegistration\" v-if=\"!loggedIn\">\n      <h1>Registration and Login</h1>\n      <br />\n      <v-container>\n        <v-layout row>\n          <v-spacer></v-spacer>\n          <v-row>\n            <v-card max-width=\"1000\" min-width=\"400\">\n              <v-card-title>Register</v-card-title>\n              <v-container>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"name_register\"\n                  label=\"Name\"\n                  required\n                ></v-text-field>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"password_register\"\n                  type=\"'password'\"\n                  label=\"Password\"\n                  required\n                ></v-text-field>\n              </v-container>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn class=\"mr-4\" @click=\"registerAccount\">Register</v-btn>\n                <v-spacer></v-spacer>\n              </v-card-actions>\n            </v-card>\n          </v-row>\n\n          <v-spacer></v-spacer>\n\n          <v-row>\n            <v-card max-width=\"1000\" min-width=\"400\">\n              <v-card-title>Login</v-card-title>\n              <v-container>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"name_login\"\n                  label=\"Name\"\n                  required\n                ></v-text-field>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"password_login\"\n                  :type=\"'password'\"\n                  label=\"Password\"\n                  required\n                ></v-text-field>\n              </v-container>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn class=\"mr-4\" @click=\"loginAccount\">Login</v-btn>\n                <v-spacer></v-spacer>\n              </v-card-actions>\n            </v-card>\n          </v-row>\n        </v-layout>\n      </v-container>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { createAccount } from \"../api/account/Account\";\nimport { login } from \"../api/account/Account\";\nimport { getStatus } from \"../api/account/Account\";\nimport { setToken } from \"../config/Token\";\nimport Search from \"@/components/Search.vue\";\n\nexport default {\n  name: \"LoginRegister\",\n\n  components: {\n    Search\n  },\n  data: () => ({\n    name_register: \"\",\n    password_register: \"\",\n    name_login: \"\",\n    password_login: \"\",\n    error: \"\",\n    loggedIn: false,\n    loggedInUserName: \"\"\n  }),\n  methods: {\n    async registerAccount() {\n      const namePass = {\n        name: this.name_register,\n        pass: this.password_register\n      };\n      await createAccount(namePass).then(\n        (this.loggedIn = true),\n        (this.loggedInUserName = this.name_register),\n        await login(namePass).then(\n          (this.loggedIn = true),\n          (this.loggedInUserName = this.name_register)\n        )\n      );\n    },\n    async loginAccount() {\n      const namePass = {\n        name: this.name_login,\n        pass: this.password_login\n      };\n      if (await login(namePass)) {\n          window.console.log('logged in'),\n          (this.loggedIn = true),\n          (this.loggedInUserName = this.name_login)\n        } else {\n          window.console.log('failed')\n        }\n\n\n      // await login(namePass).then(\n      //   window.console.log(res),\n      //   (this.loggedIn = true),\n      //   (this.loggedInUserName = this.name_login)\n      // );\n    },\n    async logOut() {\n      setToken(\"\");\n      window.location.reload();\n    }\n  },\n  async created() {\n    try {\n      let res = await getStatus();\n      this.loggedIn = true;\n      this.loggedInUserName = res.user.name;\n    } catch (err) {\n      this.loggedIn = false;\n      this.loggedInUserName = \"\";\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/components"}]}