,EAAA,OAAA,EAAA;AACA,IAAA,eADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,cAAA,QAFA,GAEA;AACA,gBAAA,IAAA,EAAA,KAAA,aADA;AAEA,gBAAA,IAAA,EAAA,KAAA;AAFA,eAFA;AAAA;AAAA,4BAMA,aAAA,CAAA,QAAA,CANA;AAAA,4BAOA,KAAA,QAAA,GAAA,IAPA;AAAA,4BAQA,KAAA,gBAAA,GAAA,KAAA,aARA;AAAA;AAAA,8CASA,KAAA,CAAA,QAAA,CAAA,CAAA,IAAA,CAAA,YAAA;AACA,qBAAA,QAAA,GAAA,IAAA;AACA,qBAAA,gBAAA,GAAA,KAAA,aAAA;AACA,eAHA,CAIA;AACA;AACA;AANA,eATA;;AAAA;AAAA;AAAA,wCAMA,IANA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA,IAAA,YAnBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBA,cAAA,QApBA,GAoBA;AACA,gBAAA,IAAA,EAAA,KAAA,UADA;AAEA,gBAAA,IAAA,EAAA,KAAA;AAFA,eApBA;AAAA;AAAA,8CAwBA,KAAA,CAAA,QAAA,CAxBA;;AAAA;AAAA;AAAA;AAAA;AAAA;;AAyBA,cAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,WAAA;AACA,mBAAA,QAAA,GAAA,IAAA;AACA,mBAAA,gBAAA,GAAA,KAAA,UAAA;AACA,mBAAA,aAAA,GAAA,KAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AA7BA;AAAA;;AAAA;AA+BA,cAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,mBAAA,aAAA,GAAA,IAAA;;AAhCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmCA,IAAA,MAnCA;AAAA;AAAA;AAAA;AAAA;AAoCA,cAAA,QAAA,CAAA,EAAA,CAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,MAAA;;AArCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAuCA,IAAA,UAvCA,wBAuCA;AACA,MAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AACA,KAzCA;AA0CA,IAAA,YA1CA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,8CA4CA,WAAA,CAAA;AAAA,gBAAA,GAAA,EAAA,KAAA,GAAA;AAAA,gBAAA,QAAA,EAAA,KAAA;AAAA,eAAA,CA5CA;;AAAA;AA4CA,cAAA,GA5CA;AAAA,gDA6CA,GA7CA;;AAAA;AAAA;AAAA;AA+CA,cAAA,MAAA,CAAA,OAAA,CAAA,GAAA;;AA/CA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GAlBA;AAqEA,EAAA,OArEA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4CAuEA,SAAA,EAvEA;;AAAA;AAuEA,YAAA,GAvEA;AAwEA,iBAAA,QAAA,GAAA,IAAA;AACA,iBAAA,gBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,YAAA,QAAA,CAAA,cAAA,CAAA,UAAA,EAAA,SAAA,GAAA,kBAAA,KAAA,gBAAA;AACA,YAAA,QAAA,CAAA,cAAA,CAAA,UAAA,EAAA,MAAA,GAAA,KAAA;AACA,YAAA,QAAA,CAAA,cAAA,CAAA,cAAA,EAAA,MAAA,GAAA,KAAA;AA5EA;AAAA;;AAAA;AAAA;AAAA;AA8EA,iBAAA,QAAA,GAAA,KAAA;AACA,iBAAA,gBAAA,GAAA,WAAA;;AA/EA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,CAAA","sourcesContent":["<template>\n  <div align=\"center\">\n     <br />\n    <div v-if=\"loggedIn\">\n      <span class=\"headline\">Logged in as {{this.loggedInUserName}}</span>\n      <v-btn style=\"float:right;\" class=\"mr-4\" @click=\"logOut\">Logout</v-btn>\n    </div>\n\n    <br />\n\n    <div class=\"LoginRegistration\" v-if=\"!loggedIn\">\n      <h1>Registration and Login</h1>\n      <br />\n      <v-container>\n        <v-layout row>\n          <v-spacer></v-spacer>\n          <v-row>\n            <v-card max-width=\"1000\" min-width=\"400\">\n              <v-card-title>Register</v-card-title>\n              <v-container>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"name_register\"\n                  label=\"Name\"\n                  required\n                ></v-text-field>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"password_register\"\n                  :type=\"'password'\"\n                  label=\"Password\"\n                  required\n                ></v-text-field>\n                <v-text-field clearable dense filled outlined v-model=\"age\" label=\"Age\" required></v-text-field>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"favoriteAlbum\"\n                  label=\"Favorite Album\"\n                  required\n                ></v-text-field>\n              </v-container>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn class=\"mr-4\" @click=\"registerAccount\">Register</v-btn>\n                <v-spacer></v-spacer>\n              </v-card-actions>\n            </v-card>\n          </v-row>\n\n          <v-spacer></v-spacer>\n\n          <v-row>\n            <v-card max-width=\"1000\" min-width=\"400\">\n              <v-card-title>Login</v-card-title>\n              <v-container>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"name_login\"\n                  label=\"Name\"\n                  required\n                ></v-text-field>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"password_login\"\n                  :type=\"'password'\"\n                  label=\"Password\"\n                  required\n                ></v-text-field>\n              </v-container>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn class=\"mr-4\" @click=\"loginAccount\">Login</v-btn>\n                <v-spacer></v-spacer>\n              </v-card-actions>\n            </v-card>\n          </v-row>\n        </v-layout>\n        <br />\n        <v-alert type=\"error\" v-if=\"loggedInError\">Error logging in</v-alert>\n      </v-container>\n    </div>\n    <!-- <v-btn class=\"mr-4\" @click=\"pushUserInfo\">register that shit</v-btn> -->\n    <Search :loggedInUserName=\"this.loggedInUserName\" />\n  </div>\n</template>\n\n<script>\nimport { createAccount } from \"../api/account/Account\";\nimport { login } from \"../api/account/Account\";\nimport { getStatus } from \"../api/account/Account\";\n// import { addUserInfo } from \"../api/account/User\";\nimport { setToken } from \"../config/Token\";\nimport Search from \"@/components/Search.vue\";\n\nexport default {\n  name: \"LoginRegister\",\n\n  components: {\n    Search\n  },\n  data: () => ({\n    name_register: \"\",\n    password_register: \"\",\n    name_login: \"\",\n    password_login: \"\",\n    error: \"\",\n    loggedIn: false,\n    loggedInUserName: \"\",\n    favoriteAlbum: \"pink moon\",\n    age: \"22\",\n    loggedInError: false\n  }),\n  methods: {\n    async registerAccount() {\n      const namePass = {\n        name: this.name_register,\n        pass: this.password_register\n      };\n      await createAccount(namePass).then(\n        (this.loggedIn = true),\n        (this.loggedInUserName = this.name_register),\n        await login(namePass).then( function () {\n          (this.loggedIn = true);\n          (this.loggedInUserName = this.name_register);\n        }\n          // (this.loggedIn = true);\n          // (this.loggedInUserName = this.name_register);\n          // document.getElementById('username');\n        )\n      );\n    },\n    async loginAccount() {\n      const namePass = {\n        name: this.name_login,\n        pass: this.password_login\n      };\n      if (await login(namePass)) {\n        window.console.log(\"logged in\");\n        this.loggedIn = true;\n        this.loggedInUserName = this.name_login;\n        this.loggedInError = false;\n        window.location.reload();\n      } else {\n        window.console.log(\"failed\");\n        this.loggedInError = true;\n      }\n    },\n    async logOut() {\n      setToken(\"\");\n      window.location.reload();\n    },\n    handleBack() {\n      window.location.reload();\n    },\n    async pushUserInfo() {\n      try {\n        let res = (await addUserInfo({age: this.age, favAlbum: this.favoriteAlbum}));\n        return res;\n      } catch(err) {\n        window.console.log(err)\n      }\n    }\n  },\n  async created() {\n    try {\n      let res = await getStatus();\n      this.loggedIn = true;\n      this.loggedInUserName = res.user.name;\n      document.getElementById('userName').innerHTML = \"Logged in as \" + this.loggedInUserName;\n      document.getElementById('userName').hidden = false;\n      document.getElementById(\"logoutButton\").hidden = false;\n    } catch (err) {\n      this.loggedIn = false;\n      this.loggedInUserName = \"Anonymous\";\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/components"}]}