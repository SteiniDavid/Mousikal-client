AAAA,GANA;AAkBA,EAAA,OAAA,EAAA;AACA,IAAA,eADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,cAAA,QAFA,GAEA;AACA,gBAAA,IAAA,EAAA,KAAA,aADA;AAEA,gBAAA,IAAA,EAAA,KAAA;AAFA,eAFA;AAAA;AAAA,4BAMA,aAAA,CAAA,QAAA,CANA;AAAA,4BAOA,KAAA,QAAA,GAAA,IAPA;AAAA,4BAQA,KAAA,gBAAA,GAAA,KAAA,aARA;AAAA;AAAA,8CASA,KAAA,CAAA,QAAA,CAAA,CAAA,IAAA,CACA,KAAA,QAAA,GAAA,IADA,EAEA,KAAA,gBAAA,GAAA,KAAA,aAFA,CATA;;AAAA;AAAA;AAAA,wCAMA,IANA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA,IAAA,YAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA,cAAA,QAhBA,GAgBA;AACA,gBAAA,IAAA,EAAA,KAAA,UADA;AAEA,gBAAA,IAAA,EAAA,KAAA;AAFA,eAhBA;AAAA;AAAA,8CAoBA,KAAA,CAAA,QAAA,CApBA;;AAAA;AAAA;AAAA;AAAA;AAAA;;AAqBA,cAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,WAAA;AACA,mBAAA,QAAA,GAAA,IAAA;AACA,mBAAA,gBAAA,GAAA,KAAA,UAAA;AACA,mBAAA,aAAA,GAAA,KAAA;AAxBA;AAAA;;AAAA;AA0BA,cAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,mBAAA,aAAA,GAAA,IAAA;;AA3BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA8BA,IAAA,MA9BA;AAAA;AAAA;AAAA;AAAA;AA+BA,cAAA,QAAA,CAAA,EAAA,CAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,MAAA;;AAhCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkCA,IAAA,YAlCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmCA,mBAAA,GAAA,GAAA,EAAA;AACA,mBAAA,aAAA,GAAA,WAAA;AApCA;AAAA;AAAA,8CAsCA,WAAA,CAAA;AACA,gBAAA,GAAA,EAAA,KAAA,GADA;AAEA,gBAAA,QAAA,EAAA,KAAA;AAFA,eAAA,CAAA,CAGA,IAHA,CAIA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,oCAAA,CAJA,EAKA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CALA,CAtCA;;AAAA;AAsCA,cAAA,GAtCA;AAAA;AAAA;;AAAA;AAAA;AAAA;AA8CA,cAAA,MAAA,CAAA,OAAA,CAAA,GAAA;;AA9CA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GAlBA;AAoEA,EAAA,OApEA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4CAsEA,SAAA,EAtEA;;AAAA;AAsEA,YAAA,GAtEA;AAuEA,iBAAA,QAAA,GAAA,IAAA;AACA,iBAAA,gBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AAxEA;AAAA;;AAAA;AAAA;AAAA;AA0EA,iBAAA,QAAA,GAAA,KAAA;AACA,iBAAA,gBAAA,GAAA,WAAA;;AA3EA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,CAAA","sourcesContent":["<template>\n  <div align=\"center\">\n    <div v-if=\"loggedIn\">\n      <span class=\"headline\">Logged in as {{this.loggedInUserName}}</span>\n      <v-btn class=\"mr-4\" @click=\"logOut\">Logout</v-btn>\n    </div>\n\n    <div class=\"LoginRegistration\" v-if=\"!loggedIn\">\n      <h1>Registration and Login</h1>\n      <br />\n      <v-container>\n        <v-layout row>\n          <v-spacer></v-spacer>\n          <v-row>\n            <v-card max-width=\"1000\" min-width=\"400\">\n              <v-card-title>Register</v-card-title>\n              <v-container>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"name_register\"\n                  label=\"Name\"\n                  required\n                ></v-text-field>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"password_register\"\n                  :type=\"'password'\"\n                  label=\"Password\"\n                  required\n                ></v-text-field>\n                <v-text-field clearable dense filled outlined v-model=\"age\" label=\"Age\" required></v-text-field>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"favoriteAlbum\"\n                  label=\"Favorite Album\"\n                  required\n                ></v-text-field>\n              </v-container>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn class=\"mr-4\" @click=\"registerAccount\">Register</v-btn>\n                <v-spacer></v-spacer>\n              </v-card-actions>\n            </v-card>\n          </v-row>\n\n          <v-spacer></v-spacer>\n\n          <v-row>\n            <v-card max-width=\"1000\" min-width=\"400\">\n              <v-card-title>Login</v-card-title>\n              <v-container>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"name_login\"\n                  label=\"Name\"\n                  required\n                ></v-text-field>\n                <v-text-field\n                  clearable\n                  dense\n                  filled\n                  outlined\n                  v-model=\"password_login\"\n                  :type=\"'password'\"\n                  label=\"Password\"\n                  required\n                ></v-text-field>\n              </v-container>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn class=\"mr-4\" @click=\"loginAccount\">Login</v-btn>\n                <v-spacer></v-spacer>\n              </v-card-actions>\n            </v-card>\n          </v-row>\n        </v-layout>\n        <br />\n        <v-alert type=\"error\" v-if=\"loggedInError\">Error logging in</v-alert>\n      </v-container>\n    </div>\n    <v-btn class=\"mr-4\" @click=\"pushUserInfo\">registerthat shit</v-btn>\n    <Search :loggedInUserName=\"this.loggedInUserName\" />\n  </div>\n</template>\n\n<script>\nimport { createAccount } from \"../api/account/Account\";\nimport { login } from \"../api/account/Account\";\nimport { getStatus } from \"../api/account/Account\";\nimport { addUserInfo } from \"../api/account/User\";\nimport { setToken } from \"../config/Token\";\nimport Search from \"@/components/Search.vue\";\n\nexport default {\n  name: \"LoginRegister\",\n\n  components: {\n    Search\n  },\n  data: () => ({\n    name_register: \"\",\n    password_register: \"\",\n    name_login: \"\",\n    password_login: \"\",\n    error: \"\",\n    loggedIn: false,\n    loggedInUserName: \"\",\n    favoriteAlbum: \"\",\n    age: \"\",\n    loggedInError: false\n  }),\n  methods: {\n    async registerAccount() {\n      const namePass = {\n        name: this.name_register,\n        pass: this.password_register\n      };\n      await createAccount(namePass).then(\n        (this.loggedIn = true),\n        (this.loggedInUserName = this.name_register),\n        await login(namePass).then(\n          (this.loggedIn = true),\n          (this.loggedInUserName = this.name_register)\n        )\n      );\n    },\n    async loginAccount() {\n      const namePass = {\n        name: this.name_login,\n        pass: this.password_login\n      };\n      if (await login(namePass)) {\n        window.console.log(\"logged in\");\n        this.loggedIn = true;\n        this.loggedInUserName = this.name_login;\n        this.loggedInError = false;\n      } else {\n        window.console.log(\"failed\");\n        this.loggedInError = true;\n      }\n    },\n    async logOut() {\n      setToken(\"\");\n      window.location.reload();\n    },\n    async pushUserInfo() {\n      this.age = 12;\n      this.favoriteAlbum = \"something\";\n      try {\n        let res = await addUserInfo({\n          age: this.age,\n          favAlbum: this.favoriteAlbum\n        }).then(\n          window.console.log(\"manged to add user info, hopefully\"),\n          window.console.log(res)\n        );\n      } catch (err) {\n        window.console.log(err);\n      }\n    }\n  },\n  async created() {\n    try {\n      let res = await getStatus();\n      this.loggedIn = true;\n      this.loggedInUserName = res.user.name;\n    } catch (err) {\n      this.loggedIn = false;\n      this.loggedInUserName = \"Anonymous\";\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/components"}]}