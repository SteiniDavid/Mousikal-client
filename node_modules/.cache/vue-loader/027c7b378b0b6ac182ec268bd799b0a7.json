th=1000 min-width=400>\n            <v-card-title>Register</v-card-title>\n            <v-container>\n              <v-text-field clearable dense filled outlined v-model=\"name_register\" label=\"Name\" required></v-text-field>\n              <v-text-field clearable dense filled outlined v-model=\"password_register\" type=\"'password'\" label=\"Password\" required></v-text-field>\n            </v-container>\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn class=\"mr-4\" @click=\"registerAccount\">Register</v-btn>\n              <v-spacer></v-spacer>\n            </v-card-actions>\n          </v-card>\n        </v-row>\n        \n        <v-spacer></v-spacer>\n\n        <v-row>\n        <v-card max-width=1000 min-width=400>\n          <v-card-title>Login</v-card-title>\n          <v-container>\n            <v-text-field clearable dense filled outlined v-model=\"name_login\" label=\"Name\" required></v-text-field>\n            <v-text-field clearable dense filled outlined v-model=\"password_login\" :type=\"'password'\" label=\"Password\" required></v-text-field>\n          </v-container>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn class=\"mr-4\" @click=\"loginAccount\">Login</v-btn>\n            <v-spacer></v-spacer>\n          </v-card-actions>\n          </v-card>\n          </v-row>\n        </v-layout>\n      </v-container>\n      </div>\n  </div>\n</template>\n\n<script>\nimport { createAccount } from \"../api/account/Account\";\nimport { login } from \"../api/account/Account\";\nimport { getStatus } from \"../api/account/Account\";\nimport { setToken } from \"../config/Token\";\nimport Search from '@/components/Search.vue'\n\nexport default {\n  name: \"LoginRegister\",\n\n  components: {\n    Search,\n  },\n\n\n\n  data: () => ({\n    name_register: \"\",\n    password_register: \"\",\n    name_login: \"\",\n    password_login: \"\",\n    error: \"\",\n    loggedIn: false,\n    loggedInUserName: \"\"\n  }),\n  methods: {\n    async registerAccount() {\n      const namePass = {\n        name: this.name_register,\n        pass: this.password_register\n      };\n      await createAccount(namePass).then(\n        (this.loggedIn = true),\n        (this.loggedInUserName = this.name_register),\n        await login(namePass).then(\n          (this.loggedIn = true),\n          (this.loggedInUserName = this.name_register)\n        )\n      );\n    },\n    async loginAccount() {\n      const namePass = {\n        name: this.name_login,\n        pass: this.password_login\n      };\n      await login(namePass).then(\n        (this.loggedIn = true),\n        (this.loggedInUserName = this.name_login)\n      );\n    },\n    async logOut() {\n      setToken(\"\");\n      window.location.reload();\n    }\n  },\n  async created() {\n    try {\n      let res = await getStatus();\n      this.loggedIn = true;\n      this.loggedInUserName = res.user.name;\n    } catch (err) {\n      this.loggedIn = false;\n      this.loggedInUserName = \"\";\n    }\n  }\n};\n</script>\n"]}]}